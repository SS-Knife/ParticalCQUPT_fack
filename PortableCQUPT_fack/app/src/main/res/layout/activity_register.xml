<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_register"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.portablecqupt_fack.register">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="200dp"
        app:srcCompat="@drawable/registerbackground"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/imageView" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_back"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/imageView12" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_register_bottom"
        android:id="@+id/imageView8"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="注册"
        android:layout_width="wrap_content"
        android:textColor="@color/white"
        android:textSize="17dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView12"
        android:layout_toRightOf="@+id/imageView12"
        android:layout_toEndOf="@+id/imageView12"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="15dp"
        android:id="@+id/textView" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_register_nickname"
        android:layout_marginTop="210dp"
        android:layout_marginLeft="20dp"
        android:id="@+id/imageView13" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_register_phone"
        android:layout_alignLeft="@+id/imageView13"
        android:layout_alignStart="@+id/imageView13"
        android:layout_marginTop="267dp"
        android:id="@+id/phone" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_register_pwd"
        android:layout_below="@id/phone"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="38dp"
        android:id="@+id/lock" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_register_gender"
        android:layout_alignBottom="@+id/spinner"
        android:layout_below="@id/lock"
        android:layout_marginTop="39dp"
        android:layout_marginLeft="19dp"
        android:id="@+id/sex" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="43dp"
        android:inputType="textPersonName"
        android:hint="请输入昵称"
        android:background="@null"
        android:ems="10"
        android:id="@+id/editText3"
        android:layout_below="@+id/imageView"
        android:layout_alignLeft="@+id/textView"
        android:layout_alignStart="@+id/textView" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="43dp"
        android:inputType="phone"
        android:hint="请输入手机号"
        android:background="@null"
        android:ems="10"
        android:id="@+id/editText5"
        android:layout_centerVertical="true"
        android:layout_below="@id/editText3"
        android:layout_toRightOf="@id/phone"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="7dp"/>
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="43dp"
        android:inputType="textPassword"
        android:hint="请输入密码"
        android:background="@null"
        android:ems="10"
        android:id="@+id/editText6"
        android:layout_below="@+id/editText5"
        android:layout_toRightOf="@id/lock"
        android:layout_marginLeft="9dp"
        android:layout_marginTop="17dp" />

    <Spinner
        android:layout_width="320dp"
        android:layout_height="30dp"
        android:entries="@array/sex"
        android:id="@+id/spinner"
        android:layout_marginTop="26dp"
        android:layout_marginLeft="9dp"
        android:layout_below="@+id/editText6"
        android:layout_toRightOf="@id/sex"
        />



    <ImageView
        android:layout_width="match_parent"
        android:background="@color/darkgrey"
        android:layout_height="2dp"
        android:layout_marginTop="310dp"
        android:id="@+id/imageView3"
        android:layout_alignLeft="@+id/imageView12"
        android:layout_alignStart="@+id/imageView12"
        android:layout_alignRight="@+id/imageView2"
        android:layout_alignEnd="@+id/imageView2" />

    <ImageView
        android:layout_width="match_parent"
        android:background="@color/darkgrey"
        android:layout_height="2dp"
        android:layout_marginTop="370dp"
        android:id="@+id/imageView4"
        android:layout_alignLeft="@+id/imageView12"
        android:layout_alignStart="@+id/imageView12"
        android:layout_alignRight="@+id/imageView3"
        android:layout_alignEnd="@+id/imageView3" />

    <Button
        android:text="登   陆"
        android:textSize="17dp"
        android:textColor="@color/white"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@drawable/signinbackground"
        android:id="@+id/button3"
        android:layout_below="@+id/editText2"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp"
        android:layout_marginTop="475dp" />



    <ImageView
        android:layout_width="340dp"
        android:background="@color/darkgrey"
        android:layout_height="2dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/imageView2"
        android:layout_marginTop="250dp"/>

    <ImageView
        android:layout_width="340dp"
        android:background="@color/darkgrey"
        android:layout_height="2dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/imageView6"
        android:layout_marginTop="430dp"
        android:layout_alignRight="@+id/imageView4"
        android:layout_alignEnd="@+id/imageView4" />

</RelativeLayout>
